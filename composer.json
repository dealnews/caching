{
  "name": "dealnews/caching",
  "type": "library",
  "license": "BSD-3-Clause",
  "description": "Caching PHP Library (Memcache, Redis, etc.)",
  "require-dev": {
    "php-parallel-lint/php-parallel-lint": "^1.4",
    "phpunit/phpunit": "^11",
    "friendsofphp/php-cs-fixer": "^3.88"
  },
  "require": {
    "php": "^8.2",
    "predis/predis": "^3.2",
    "dealnews/get-config": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "DealNews\\Caching\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DealNews\\Caching\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": [
      "@lint",
      "phpunit --colors=never"
    ],
    "lint": [
      "parallel-lint src/ tests/"
    ],
    "fix": [
      "php-cs-fixer fix --config .php-cs-fixer.dist.php src tests"
    ]
  }
}
